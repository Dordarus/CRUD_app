<%= provide(:title, @user.name)%>
<div class="container text-center">
    <div class="page-header">
        <h1>Profile</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-2 col-md-offset-5">
        <ul class="list-group">
            <li class="list-group-item">Your name : <%= @user.name %></li>
            <li class="list-group-item">Your Email : <%= @user.email %></li>
            <li class="list-group-item">Account created at : <%= @user.created_at %></li>
        </ul> 
    </div>
</div>